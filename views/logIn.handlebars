 <main class="index">
    <div class="index--container container">
      <header class="index--header">
        <h1><b>Welcome to Vanessa's personal website</b></h1>
        <h1><b>Log In to Access (●'◡'●)</b></h1>
      <div >
        <label class="h3"><b>Username:</b></label>
        <input type="text" id="username" class="form-control input-lg" placeholder="username" /></span>
      </div>
      <div >
        <label class="h3"><b>Password:</b></label>
        <input type="password" id="password" class="form-control input-lg" placeholder="password" />
      </div>
      <div class="index--btn-container">
        <button class="btn btn-primary btn-sm" id="login-btn"><b>Log In</b></button>
        <button class="btn btn-success btn-sm" id="register-btn"><b>Register</b></button>
        <button class="btn btn-warning btn-sm" id="update-btn"><b>Update User</b></button>
        <button class="btn btn-danger  btn-sm" id="delete-btn"><b>Delete User</b></button>
        <!-- <button class="btn btn-danger  btn-sm" id="logout-btn"><b>Log Out </b></button> -->
      </div>
      <div class="h4"><b>Current user: </b><span id="current-user"></span></div>
       {{!-- {{#if error}}
        <div class="h4 text-danger"><b>Error: {{error}}</b></div>
      {{/if}} --}}
      <div id="all-users" class="h4"><b>Users: {{#each users}}{{this}},{{/each}}</b></div>
    </div>
    <div id="notification"></div>
 </main>